generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model FinancialRecord {
  id             Int      @id @default(autoincrement())
  date           DateTime
  revenue        Int
  expenses       Int
  profit         Int
  customerCount  Int
  
  @@map("financial_records")
}